#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    int t,count=0;
    double r=1,w,h;
    scanf("%d",&t);
//     for(int i=0;i<t;i++)
//     {
        
//     }
    for(int j=0;j<t;j++)
    {
        scanf("%d%d",&w,&h);
        r=w/h;
        if(r>=1.6 && r<=1.7)
            count++;
    }
    printf("%d",count);

    return 0;
}
